<?xml version="1.0" encoding="utf-8"?>
<selector xmlns:android="http://schemas.android.com/apk/res/android">
    <item android:state_selected="true">
        <layer-list>
            <item android:width="64dp" android:height="32dp" android:gravity="center">
                <shape android:shape="rectangle">
                    <solid android:color="@color/white" />
                    <corners android:radius="48dp" />
                </shape>
            </item>
            <item android:width="17.5dp" android:height="20dp" android:drawable="@drawable/bottom_nav_logbook" android:gravity="center" />
        </layer-list>
    </item>
    <item android:drawable="@drawable/bottom_nav_logbook" android:state_selected="false"/>

</selector>